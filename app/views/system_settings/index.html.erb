<h1>Listing system_settings</h1>

<table>
  <tr>
    <th>Event</th>
    <th>Min time diff</th>
    <th>Break time</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @system_settings.each do |system_setting| %>
  <tr>
    <td><%= system_setting.event_id %></td>
    <td><%= system_setting.min_time_diff %></td>
    <td><%= system_setting.break_time %></td>
    <td><%= link_to 'Show', system_setting %></td>
    <td><%= link_to 'Edit', edit_system_setting_path(system_setting) %></td>
    <td><%= link_to 'Destroy', system_setting, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

